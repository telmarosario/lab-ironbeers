<div>

  <h1>Random Beer</h1>

  <div>
    {{#each aBeerKey}}
    {{> beerpartial this }}
      <span>Food Pairing</span>
      <ul>
        {{#each food_pairing}}
          <li>{{this}}</li>
        {{/each}}
      </ul>
      <p>Brewers Tips: {{this.brewers_tips}}</p>
    {{/each}}
  </div>

</div>